<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.10120908UTS" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/clam_shell</item>
        <item name="colorPrimaryVariant">@color/spicy_pink</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="roundedCornerImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>

    <style name="roundedCorner24ImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/_24sdp</item>
    </style>

    <style name="roundedCornerBottomImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomLeft">@dimen/_24sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_24sdp</item>
    </style>

    <style name="roundedCornerTopImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">@dimen/_24sdp</item>
        <item name="cornerSizeTopRight">@dimen/_24sdp</item>
    </style>

    <style name="ButtonDefault" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@color/wafer</item>
        <item name="android:textColor">@color/spicy_pink</item>
        <item name="android:textAppearance">@style/TextAppearance.Roboto.Regular.Button</item>
        <item name="cornerRadius">@dimen/request_option_circle_radius</item>
    </style>

    <style name="ThemeOverlay.MaterialComponents.MaterialAlertDialog" parent="Base.ThemeOverlay.MaterialComponents.MaterialAlertDialog" tools:ignore="PrivateResource">
        <item name="materialAlertDialogTitlePanelStyle">
            @style/MaterialAlertDialog.MaterialComponents.Title.Panel
